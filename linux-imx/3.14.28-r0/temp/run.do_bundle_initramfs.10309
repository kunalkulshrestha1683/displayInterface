do_bundle_initramfs() {
	if [ ! -z "" -a x"" = x1 ]; then
		echo "Creating a kernel image with a bundled initramfs..."
		copy_initramfs
		if [ -e arch/arm/boot/zImage ] ; then
			mv -f arch/arm/boot/zImage arch/arm/boot/zImage.bak
		fi
		use_alternate_initrd=CONFIG_INITRAMFS_SOURCE=/home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.cpio
		kernel_do_compile
		mv -f arch/arm/boot/zImage arch/arm/boot/zImage.initramfs
		mv -f arch/arm/boot/zImage.bak arch/arm/boot/zImage
		# Update install area
		echo "There is kernel image bundled with initramfs: /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/arch/arm/boot/zImage.initramfs"
		install -m 0644 /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/arch/arm/boot/zImage.initramfs /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/image/boot/zImage-initramfs-imx6dlsabreauto.bin
		echo "/home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/arch/arm/boot/zImage.initramfs"
	fi

}

copy_initramfs() {
	echo "Copying initramfs into ./usr ..."
	# In case the directory is not created yet from the first pass compile:
	mkdir -p /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr
	# Find and use the first initramfs image archive type we find
	rm -f /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.cpio
	for img in cpio.gz cpio.lz4 cpio.lzo cpio.lzma cpio.xz; do
		if [ -e "/home/kunalk/fsl-release-bspdl/build/tmp/deploy/images/imx6dlsabreauto/-imx6dlsabreauto.$img" ]; then
			cp /home/kunalk/fsl-release-bspdl/build/tmp/deploy/images/imx6dlsabreauto/-imx6dlsabreauto.$img /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/.
			case $img in
			*gz)
				echo "gzip decompressing image"
				gunzip -f /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.$img
				break
				;;
			*lz4)
				echo "lz4 decompressing image"
				lz4 -df /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.$img
				break
				;;
			*lzo)
				echo "lzo decompressing image"
				lzop -df /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.$img
				break
				;;
			*lzma)
				echo "lzma decompressing image"
				lzma -df /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.$img
				break
				;;
			*xz)
				echo "xz decompressing image"
				xz -df /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.$img
				break
				;;
			esac
		fi
	done
	echo "Finished copy of initramfs into ./usr"

}

kernel_do_compile() {
	unset CFLAGS CPPFLAGS CXXFLAGS LDFLAGS MACHINE
	# The $use_alternate_initrd is only set from
	# do_bundle_initramfs() This variable is specifically for the
	# case where we are making a second pass at the kernel
	# compilation and we want to force the kernel build to use a
	# different initramfs image.  The way to do that in the kernel
	# is to specify:
	# make ...args... CONFIG_INITRAMFS_SOURCE=some_other_initramfs.cpio
	if [ "$use_alternate_initrd" = "" ] && [ "" != "" ] ; then
		# The old style way of copying an prebuilt image and building it
		# is turned on via INTIRAMFS_TASK != ""
		copy_initramfs
		use_alternate_initrd=CONFIG_INITRAMFS_SOURCE=/home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/usr/-imx6dlsabreauto.cpio
	fi
	oe_runmake zImage  CC="arm-poky-linux-gnueabi-gcc  -mno-thumb-interwork -marm" LD="arm-poky-linux-gnueabi-ld.bfd  "  LOADADDR=0x10008000 $use_alternate_initrd
	if test "zImage.gz" = "zImage"; then
		gzip -9c < "zImage" > "arch/arm/boot/zImage"
	fi

}

oe_runmake() {
	oe_runmake_call "$@" || die "oe_runmake failed"

}

die() {
	bbfatal "$*"

}

oe_runmake_call() {
	bbnote make  "$@"
	make  "$@"

}

bbfatal() {
	echo "ERROR: $*"
	exit 1

}

bbnote() {
	echo "NOTE: $*"

}

cd '/home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git'
do_bundle_initramfs

# cleanup
ret=$?
trap '' 0
exit $?
