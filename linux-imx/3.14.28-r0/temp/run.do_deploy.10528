do_deploy() {
    kernel_do_deploy
	if test -n "imx6dl-sabreauto.dtb imx6dl-sabreauto-gpmi-weim.dtb imx6dl-sabreauto-ecspi.dtb  imx6dl-sabreauto-flexcan1.dtb "; then
		for DTB in imx6dl-sabreauto.dtb imx6dl-sabreauto-gpmi-weim.dtb imx6dl-sabreauto-ecspi.dtb  imx6dl-sabreauto-flexcan1.dtb ; do
			if echo ${DTB} | grep -q '/dts/'; then
				bbwarn "${DTB} contains the full path to the the dts file, but only the dtb name should be used."
				DTB=`basename ${DTB} | sed 's,\.dts$,.dtb,g'`
			fi
			DTB_BASE_NAME=`basename ${DTB} .dtb`
			DTB_NAME=`echo zImage--3.14.28-r0-imx6dlsabreauto-20170104130301 | sed "s/imx6dlsabreauto/${DTB_BASE_NAME}/g"`
			DTB_SYMLINK_NAME=`echo zImage-imx6dlsabreauto | sed "s/imx6dlsabreauto/${DTB_BASE_NAME}/g"`
			DTB_PATH="/home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/arch/arm/boot/dts/${DTB}"
			if [ ! -e "${DTB_PATH}" ]; then
				DTB_PATH="/home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git/arch/arm/boot/${DTB}"
			fi
			install -d /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx
			install -m 0644 ${DTB_PATH} /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx/${DTB_NAME}.dtb
			cd /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx
			ln -sf ${DTB_NAME}.dtb ${DTB_SYMLINK_NAME}.dtb
			cd -
		done
	fi

}

kernel_do_deploy() {
	install -m 0644 arch/arm/boot/zImage /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx/zImage--3.14.28-r0-imx6dlsabreauto-20170104130301.bin
	if [ 1 = "1" ] && (grep -q -i -e '^CONFIG_MODULES=y$' .config); then
		mkdir -p /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/image/lib
		tar -cvzf /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx/modules--3.14.28-r0-imx6dlsabreauto-20170104130301.tgz -C /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/image lib
		ln -sf modules--3.14.28-r0-imx6dlsabreauto-20170104130301.tgz /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx/modules-imx6dlsabreauto.tgz
	fi

	ln -sf zImage--3.14.28-r0-imx6dlsabreauto-20170104130301.bin /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx/zImage-imx6dlsabreauto.bin
