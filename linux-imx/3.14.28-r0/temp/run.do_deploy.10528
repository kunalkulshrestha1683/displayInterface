	cp /home/kunalk/fsl-release-bspdl/sources/poky/meta/files/deploydir_readme.txt /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx/README_-_DO_NOT_DELETE_FILES_IN_THIS_DIRECTORY.txt

	cd /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/git
	# Update deploy directory
	if [ -e "arch/arm/boot/zImage.initramfs" ]; then
		echo "Copying deploy kernel-initramfs image and setting up links..."
		initramfs_base_name=zImage-initramfs-3.14.28-r0-imx6dlsabreauto-20170104130301
		initramfs_symlink_name=zImage-initramfs-imx6dlsabreauto
		install -m 0644 arch/arm/boot/zImage.initramfs /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx/${initramfs_base_name}.bin
		cd /home/kunalk/fsl-release-bspdl/build/tmp/work/imx6dlsabreauto-poky-linux-gnueabi/linux-imx/3.14.28-r0/deploy-linux-imx
		ln -sf ${initramfs_base_name}.bin ${initramfs_symlink_name}.bin
	fi

